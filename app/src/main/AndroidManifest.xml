<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SurfaceViewTestDemo"
        tools:targetApi="31">
        <activity
            android:name=".mmap.MMapDemoActivity"
            android:exported="false" />
        <activity
            android:name=".OpenGLFilterActivity"
            android:exported="false" />
        <activity
            android:name=".webrtc.WebRTCDemoActivity"
            android:exported="false" />
        <activity
            android:name=".audio.AudioFileTransformActivity"
            android:exported="false" />
        <activity
            android:name=".camera.CameraActivity"
            android:exported="false" />
        <activity
            android:name=".reentrantlock.ReentrantLockTestActivity"
            android:exported="false" />
        <activity
            android:name=".broadcast.BroadcastActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".broadcast.MyReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".launch_mode.SecondActivity"
            android:exported="false"
            android:launchMode="singleTask" />
        <activity
            android:name=".launch_mode.FirstActivity"
            android:exported="true" />
        <activity
            android:name=".scroll_conflict.ScrollConflictActivity"
            android:exported="true" />
        <activity
            android:name=".view_basic.TouchEventDispatchActivity"
            android:exported="true" />
        <activity
            android:name=".view_basic.VelocityTrackerActivity"
            android:exported="true" />
        <activity
            android:name=".view_basic.TouchEventActivity"
            android:exported="true" />
        <activity
            android:name=".view_basic.TestLayoutParamsActivity"
            android:exported="true" />
        <activity
            android:name=".surface.SurfaceADumpActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SurfaceViewTestDemo" />
    </application>

</manifest>